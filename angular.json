{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "his-doctor": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/his-doctor",
      "sourceRoot": "projects/his-doctor/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/his-doctor",
            "index": "projects/his-doctor/src/index.html",
            "main": "projects/his-doctor/src/main.ts",
            "polyfills": "projects/his-doctor/src/polyfills.ts",
            "tsConfig": "projects/his-doctor/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/his-doctor/src/favicon.ico",
              "projects/his-doctor/src/assets"
            ],
            "styles": [
              "projects/his-doctor/src/styles.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
			        "node_modules/datatables.net-dt/css/jquery.dataTables.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",              
              "node_modules/datatables.net/js/jquery.dataTables.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/his-doctor/src/environments/environment.ts",
                  "with": "projects/his-doctor/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "his-doctor:build",
            "proxyConfig": "projects/his-doctor/src/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "his-doctor:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "his-doctor:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/his-doctor/src/test.ts",
            "polyfills": "projects/his-doctor/src/polyfills.ts",
            "tsConfig": "projects/his-doctor/tsconfig.spec.json",
            "karmaConfig": "projects/his-doctor/karma.conf.js",
            "assets": [
              "projects/his-doctor/src/favicon.ico",
              "projects/his-doctor/src/assets"
            ],
            "styles": [
              "projects/his-doctor/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/his-doctor/tsconfig.app.json",
              "projects/his-doctor/tsconfig.spec.json",
              "projects/his-doctor/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/his-doctor/e2e/protractor.conf.js",
            "devServerTarget": "his-doctor:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "his-doctor:serve:production"
            }
          }
        }
      }
    }	
  },
  "defaultProject": "his-doctor",
  "cli": {
    "analytics": "84b399eb-7fee-41ff-8a18-22d9baad3bba"
  }
}